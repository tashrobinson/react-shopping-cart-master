  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "chai": "^4.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "fetch-mock": "^7.7.3",
    "firebase-tools": "^6.12.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.4",
    "sinon": "^7.5.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
